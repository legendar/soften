<?xml version="1.0" encoding="UTF-8"?>
<sitemap>

    <match pattern="/(js|javascript)/">
	    <file var="js[]" dir="js" src="*.js"/>
        <dir var="{SITEPATH}"/>
        <file var="js[]" dir="js" src="*.js"/>
        <dir var="{ENGINEPATH}" />
	    <template src="js.tpl.php"/>
	    <end/>
	</match>
    
    <match pattern="/js/(prototype|mootools|scriptaculous|jquery|phpjs)/">
        <file var="js[]" dir="js" src="{1}/*.js"/>
	    <template src="js.tpl.php"/>
	    <end/>
	</match>
    
    <!--<match pattern="/js/scriptaculous/">
        <file var="js[]" dir="js" src="{1}/*.js"/>
	    <template src="js.tpl.php"/>
	    <end/>
	</match>-->
    
</sitemap>